{
  "name": "notebot",
  "version": "1.0.0",
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "src/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "scripts": {
    "start": "npm run dev",
    "build": "npm-run-all  --sequential clean transpile server",
    "server": "node ./dist-server/server.js",
    "dev": "NODE_ENV=development npm-run-all build",
    "prod": "NODE_ENV=production npm-run-all build server",
    "transpile": "babel ./src  --out-dir dist-server --source-maps",
    "clean": "rimraf dist-server",
    "watch:dev": "nodemon"
  },
  "dependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "dotenv": "^16.0.3",
    "eureka-js-client": "^4.5.0",
    "express": "^4.18.2",
    "mongoose": "^7.6.6",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.0",
    "socket.io": "^4.5.1"
  },
  "optionalDependencies": {
    "win-node-env": "^0.6.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
